{
  "name": "widget",
  "version": "1.3.7",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --open --mode localhost",
    "serve": "vue-cli-service serve",
    "build:localhost": "vue-cli-service build --mode localhost",
    "build:dev": "vue-cli-service build --mode dev",
    "build:prod": "vue-cli-service build --mode prod",
    "widget:localhost": "vue-cli-service build --target wc --inline-vue --name widget-web --mode localhost",
    "widget:dev": "vue-cli-service build --target wc --inline-vue --name widget-web --mode dev",
    "widget:prod": "vue-cli-service build --target wc --inline-vue --name widget-web --mode prod",
    "lint": "vue-cli-service lint",
    "build": "node bump-version.js && npx webpack --config webpack.config.js"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.8.4",
    "vue": "^3.5.16",
    "vuetify": "^3.7.19",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.5.16",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "^10.0.0",
    "sass": "~1.86.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "vue-loader": "^17.4.2",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-plugin-vuetify": "^3.1.0"
  }
}